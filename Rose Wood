<script>
$(document).ready(function(){
    {block:IndexPage}
    $(".npf-audio-wrapper,.spotify_audio_player,.soundcloud_audio_player ").each(function() {
        $(this).not(".modal-container .npf-audio-wrapper,.modal-container .spotify_audio_player,.modal-container .soundcloud_audio_player").replaceWith("<div class='small'><span class='bx bxs-music'></span></div>");
    });
    var $grid = $(".grid").imagesLoaded( function() {
        $(".grid").masonry({
          itemSelector:"article",
          stagger:30,
          visibleStyle:{ transform:"translateY(0)", opacity: 1 },
          hiddenStyle:{ transform:"translateY(100px)", opacity: 0 }
        });
    });
    {block:ifInfiniteScroll}
    var $grid = $(".grid").masonry({
        itemSelector:"article",
        stagger:30,
        visibleStyle:{ transform:"translateY(0)", opacity: 1 },
        hiddenStyle:{ transform:"translateY(100px)", opacity: 0 }
    });
    var msnry = $grid.data("masonry");
    $grid.infiniteScroll({
        path:".next a",
        append:"article",
        status:".load-status",
        responseBody:"text",
        history:false,
        outlayer:msnry,
        hideNav:".pgs",
        button:".load",
        {block:ifManualLoad}scrollThreshold:false{/block:ifManualLoad}
        {block:ifNotManualLoad}scrollThreshold:true{/block:ifNotManualLoad}
    });
    $grid.on("append.infiniteScroll", function(event, response, path, items) {
    var $elem = $(items);
    $elem.imagesLoaded( function() {
        $grid.infiniteScroll(items)
        .masonry(items);
    });
    var $likes = $elem.map(function () { return this.id;}).get();
        Tumblr.LikeButton.get_status_by_post_ids($likes);
    $(".permalink").each(function(){
        $(this).click(function(){
            $(this).closest(items).find(".modal").fadeIn();
        });
    });
    $(".exit,.modal-bg").each(function(){
        $(this).click(function(){
            $(this).closest(items).find(".modal").fadeOut();
        });
    });
    $(".npf-audio-wrapper,.spotify_audio_player,.soundcloud_audio_player ").each(function() {
        $(this).not(".modal-container .npf-audio-wrapper,.modal-container .spotify_audio_player,.modal-container .soundcloud_audio_player").replaceWith("<div class='small'><span class='bx bxs-music'></span></div>");
    });
    $(".username + p").filter(function() { return $(this).text().trim().length == 0; }).remove();
    $(".reblogged a.read_more").each(function(){
        $(this).wrapAll("<div class='read_more_contain'></div>");
    });
    $("p:not(:only-of-type).npf_chat").closest("article").wrapAll("<div class='not-only'></div>");
    $(".npf_inst ~ p.npf_chat,blockquote ~ p.npf_chat,ul ~ p.npf_chat,ol ~ .npf_chat").each(function(){
        $(this).wrapAll("<div class='below'></div>");
    });
        tumblr_npf_audio({
        emptyTitleText:"",
        emptyArtistText:"",
        emptyAlbumText:"",
        titleLabel:"",
        artistLabel:"",
        albumLabel:""
        });
        customAudio({
	    post:"article",
	    wrappers: {
		    audio:".audio",
		    buttons:".audio-btns",
    		seekbar:".seekbar"
	    },
	        default:false,
	        pauseAll:true,
	        playButton:"<i class='bx bx-play'></i>",
	        pauseButton:"<i class='bx bx-pause'></i>",
	        errorIcon:"<i class='bx bx-play'></i>",
	        hideInfoIfError:true
        });
        tippy("[title]", {
            theme:"custom",
            duration:300,
            allowHTML:true,
            arrow:false,
            followCursor:true,
            inlinePositioning:true,
            placement:"auto",
            zIndex:9999999,
            content(reference) {
                const title = reference.getAttribute("title");
                reference.removeAttribute("title");
                return title;
            },
        });
    });
    {/block:ifInfiniteScroll}
    {/block:IndexPage}
});
</script>

