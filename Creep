<script>
$(document).ready(function(){
    var $container = $("main");
    $("a[title]").style_my_tooltips({
        tip_follows_cursor:true,
        tip_delay_time:200,
        tip_fade_speed:300,
        attribute:"title"
    });
    var $audio = $('iframe.tumblr_audio_player');
    $audio.load(function() {
    $(this).contents().find('head').append('<style type="text/css">' +
      '.audio-player { background:{color:Accent 1}; color:{color:Accent 2}; border-bottom:1px solid {color:Borders}!important; }' + '.audio-player .audio-info .track-artist { color:{color:Accent 2} !important; }' + '</style>');
    });
    $(".photo-slideshow").pxuPhotoset({
        lightbox:true,
        rounded:false,
        gutter:"5px",
        borderRadius:"0px",
        photoset:".photo-slideshow",
        photoWrap:".photo-data",
        photo:".pxu-photo"
    });
    {block:ifGrid}
    $(window).load(function () {
    $("section").masonry({
    itemSelector :"article",
    },
    function() { $("section").masonry({ appendedContent:$(this) }); }
    );
    });
    {/block:ifGrid}
});
</script>

